# API Configuration
VITE_API_BASE_URL=http://localhost:3000/api

# LangChain.js AI Model API Keys
# OpenAI API (for ChatGPT models)
VITE_OPENAI_API_KEY=your_openai_api_key_here

# Anthropic API (for Claude models)
VITE_ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Google AI API (for Gemini models)
VITE_GOOGLE_API_KEY=your_google_api_key_here

# Cohere API (for Command models)
VITE_COHERE_API_KEY=your_cohere_api_key_here

# App Configuration
VITE_APP_NAME=CELPIP Prep
VITE_APP_VERSION=1.0.0

# Feature Flags
VITE_ENABLE_AI_GENERATION=true
VITE_ENABLE_AUDIO_RECORDING=false
VITE_ENABLE_PROGRESS_TRACKING=true
